<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.softtechnotech.zerokata">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".multi_player"
            android:screenOrientation="portrait"/>
        <activity android:name=".StartPage"
            android:screenOrientation="portrait"/>
        <activity android:name=".splashscreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".highscores"
            android:screenOrientation="portrait"/>
        <activity android:name=".multiplayerlevels"
            android:screenOrientation="portrait"/>
        <activity android:name=".multi_player_easy"
            android:screenOrientation="portrait"/>
        <activity android:name=".multi_player_hard"
            android:screenOrientation="portrait"/>
        <activity android:name=".singleplayerlevels"
            android:screenOrientation="portrait"/>
        <activity android:name=".single_player_easy"
            android:screenOrientation="portrait"/>
        <activity android:name=".single_player_AI"
            android:screenOrientation="portrait"/>
        <activity android:name=".single_player_hard"
            android:screenOrientation="portrait"/>
        <service android:name=".ServDsp" android:enabled="true"/>
        <service android:name=".ServGot" android:enabled="true"/>
        <service android:name=".ServFad" android:enabled="true"/>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5803830864440853~1685417265"/>
    </application>

</manifest>